name = Tenjin

# The location of the AIR SDK 
air.sdk = path/to/your/AIR/SDK

# The SWF version to compile against
swf.version = 27

# Executable files extensions (for Mac leave empty, for Windows, set to .exe and .bat)
bin.ext = .exe
bat.ext = .bat

# Location of the Android SDK
android.sdk = path/to/android/sdk/folder

# Location of the iOS SDK
ios.sdk = /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS9.2.sdk

# Using the "export" target will copy the ANE file to this location
export.dir = /path/to/where/you/want/to/export/the/ANE/file